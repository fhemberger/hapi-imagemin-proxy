{
  "name": "hapi-imagemin-proxy",
  "version": "1.1.3",
  "description": "Hapi proxy for serving optimized images",
  "author": "Frederic Hemberger (https://frederic-hemberger.de/)",
  "repository": "https://github.com/fhemberger/hapi-imagemin-proxy.git",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lab": "NODE_ENV=test lab -L --lint-warnings-threshold 99999 -I __RESOLVED_TMP_DIR__",
    "test": "npm run lab -- test/*.js",
    "coverage": "npm run lab -- -c",
    "coverage:html": "npm run coverage -- -r html -o coverage.html",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "hapi",
    "image",
    "images",
    "optimize",
    "imageoptim",
    "imagemin",
    "imagemagick",
    "graphicsmagick",
    "jpg",
    "jpeg",
    "png",
    "gif",
    "svg",
    "proxy"
  ],
  "dependencies": {
    "boom": "^5.1.0",
    "catbox": "^7.1.5",
    "catbox-memory": "^2.0.4",
    "generaterr": "^1.5.0",
    "hoek": "^4.1.1",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-gm": "^1.0.3",
    "imagemin-jpegoptim": "^5.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.1",
    "joi": "^11.0.1",
    "ms": "^2.0.0",
    "promise.pipe": "^3.0.0",
    "wreck": "^12.2.0",
    "xregexp": "^3.2.0",
    "snyk": "^1.30.1"
  },
  "devDependencies": {
    "code": "^5.1.0",
    "eslint-config-hapi": ">=10.0.0",
    "eslint-plugin-hapi": ">=4.x.x",
    "hapi": "^16.5.2",
    "lab": "^14.1.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.8.0",
    "sinon": "^3.2.1"
  },
  "snyk": true
}
