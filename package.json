    "hoek": "4.x.x",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-gm": "^1.0.0",
    "imagemin-jpegoptim": "^5.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.1.0",
    "joi": "9.x.x",
    "ms": "^0.7.1",
    "promise.pipe": "^3.0.0",
