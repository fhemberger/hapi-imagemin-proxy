{
  "name": "hapi-imageoptimizer",
  "version": "0.0.9",
  "description": "Proxy for serving optimized images",
  "author": "Frederic Hemberger (https://frederic-hemberger.de/)",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "start": "node server.js 2>&1",
    "lab": "NODE_ENV=test node node_modules/.bin/lab -L --lint-warnings-threshold 99999",
    "test": "npm run lab -- test/*.js",
    "coverage": "npm run lab -- -c",
    "coverage:html": "npm run coverage -- -r html -o coverage.html"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "keywords": [
    "hapi",
    "image",
    "images",
    "optimize",
    "imageoptim",
    "jpg",
    "jpeg",
    "png",
    "gif",
    "svg",
    "proxy"
  ],
  "dependencies": {
    "boom": "^3.1.2",
    "catbox": "^7.1.0",
    "catbox-memory": "^2.0.1",
    "config": "^1.19.0",
    "generaterr": "^1.3.0",
    "gifsicle": "3.0.3",
    "gm": "^1.21.1",
    "good": "^6.6.0",
    "good-console": "^5.3.1",
    "hapi": "^13.0.0",
    "hoek": "^3.0.1",
    "joi": "^8.0.5",
    "jpegoptim-bin": "3.0.0",
    "optipng-stream-bin": "0.0.7",
    "wreck": "^7.1.0",
    "xregexp": "^3.1.0"
  },
  "devDependencies": {
    "code": "^2.1.0",
    "eslint-config-hapi": "^9.1.0",
    "eslint-plugin-hapi": "^4.0.0",
    "lab": "^10.3.0",
    "proxyquire": "^1.7.4"
  }
}
